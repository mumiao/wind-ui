{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": true
      }
    ],
    "transform-vue-jsx",
    [
      "import",
      {
        "libraryName": "wind-ui",
        "libraryDirectory": "lib/components",
        "style": true,
      }
    ]
  ]
}